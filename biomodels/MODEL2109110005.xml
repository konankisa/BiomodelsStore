    <listOfRules>
      <assignmentRule variable="CAR_T_cells">
        <math xmlns="http://www.w3.org/1998/Math/MathML">        
          <apply>
            <plus/>
            <ci> C_E </ci>
            <ci> C_P </ci>
          </apply>
        </math>
            </assignmentRule>
      <assignmentRule variable="F_cure">
        <math xmlns="http://www.w3.org/1998/Math/MathML">        
          <apply>
            <divide/>
            <cn> 1 </cn>
            <apply>
              <plus/>
              <cn> 1 </cn>
              <apply>
                <power/>
                <apply>
                  <divide/>
                  <ci> U_crit </ci>
                  <ci> U </ci>
                </apply>
                <ci> h </ci>
              </apply>
            </apply>
          </apply>
        </math>
            </assignmentRule>
      <rateRule variable="C_E">
        <math xmlns="http://www.w3.org/1998/Math/MathML">        
          <piecewise>
            <piece>
              <apply>
                <times/>
                <ci> rho </ci>
                <ci> C_E </ci>
              </apply>
              <apply>
                <leq/>
                <csymbol encoding="text" definitionURL="http://www.sbml.org/sbml/symbols/time"> time </csymbol>
                <ci> T_max </ci>
              </apply>
            </piece>
            <otherwise>
              <apply>
                <times/>
                <apply>
                  <minus/>
                  <apply>
                    <plus/>
                    <ci> k_EP </ci>
                    <ci> k_E </ci>
                  </apply>
                </apply>
                <ci> C_E </ci>
              </apply>
            </otherwise>
          </piecewise>
        </math>
            </rateRule>
      <rateRule variable="C_P">
        <math xmlns="http://www.w3.org/1998/Math/MathML">        
          <piecewise>
            <piece>
              <cn> 0 </cn>
              <apply>
                <leq/>
                <csymbol encoding="text" definitionURL="http://www.sbml.org/sbml/symbols/time"> time </csymbol>
                <ci> T_max </ci>
              </apply>
            </piece>
            <otherwise>
              <apply>
                <minus/>
                <apply>
                  <times/>
                  <ci> k_EP </ci>
                  <ci> C_E </ci>
                </apply>
                <apply>
                  <times/>
                  <ci> beta </ci>
                  <ci> C_P </ci>
                </apply>
              </apply>
            </otherwise>
          </piecewise>
        </math>
            </rateRule>
    </listOfRules>
